{
  "name": "lab-report-ingestion-system",
  "version": "1.0.0",
  "description": "Automated MDR detection and alerting system",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "worker": "node src/workers/notificationWorker.js",
    "test": "node tests/lab-report-tests.js",
    "setup-db": "mysql -u root -p hospital_db < database/lab-reports-schema.sql",
    "start-all": "concurrently \"npm start\" \"npm run worker\""
  },
  "dependencies": {
    "express": "^4.18.2",
    "mysql2": "^3.6.0",
    "express-validator": "^7.0.1",
    "ioredis": "^5.3.2",
    "ws": "^8.14.2",
    "firebase-admin": "^11.11.0",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  }
}
